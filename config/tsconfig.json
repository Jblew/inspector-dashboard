{
    "compilerOptions": {
        "declaration": true,
        "sourceMap": true,
        "module": "commonjs",
        "target": "es5",
        "strict": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "allowJs": false,
        "outDir": "dist",
        "baseUrl": "./"
    },
    "include": ["./*.ts"],
    "exclude": ["node_modules"],
    "references": []
}
